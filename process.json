{
  "apps": [
    {
      "name": "CiscoJabberEvents",
      "cwd": "/usr/local/node/ciscojabberevents/src",
      "script": "./index.js",
      "exec_mode": "fork",
      "combine_logs": true,
      "merge_logs": true,
      "error_file": "/var/log/ciscoojabberevents-err.log",
      "out_file": "/dev/null",
      "pid_file": "/var/run/ciscojabberevents.pid",
      "log_date_format": "MMM DD HH:mm:ss",
      "env": {
        "watch": false,
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production",
        "watch": false
      }
    }
  ]
}
